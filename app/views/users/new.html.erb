<%= form_for(@user) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password%>
    
    <%= f.submit %>

<% end %> 
<p><%= link_to 'Sign up in with the GitHub', '/auth/github'%>.</p>